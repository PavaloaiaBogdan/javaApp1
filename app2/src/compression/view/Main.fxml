<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="root" prefHeight="500.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="compression.model.MainController">
   <children>
      <BorderPane layoutX="-8.0" prefHeight="400.0" prefWidth="650.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <top>
            <VBox>
               <children>
                  <MenuBar BorderPane.alignment="CENTER">
                    <menus>
                      <Menu mnemonicParsing="false" text="File">
                        <items>
                          <MenuItem mnemonicParsing="false" text="Open" />
                              <MenuItem mnemonicParsing="false" text="Close" />
                        </items>
                      </Menu>
                      <Menu mnemonicParsing="false" text="Edit">
                        <items>
                          <MenuItem fx:id="men1" mnemonicParsing="false" onAction="#handleMenuAction" text="Scene1" />
                              <MenuItem fx:id="men2" mnemonicParsing="false" onAction="#handleMenuAction" text="Scene2" />
                              <MenuItem fx:id="men3" mnemonicParsing="false" onAction="#handleMenuAction" text="Scene3" />
                        </items>
                      </Menu>
                      <Menu mnemonicParsing="false" text="Help">
                        <items>
                          <MenuItem mnemonicParsing="false" text="Algorithms" />
                              <MenuItem mnemonicParsing="false" text="About" />
                        </items>
                      </Menu>
                    </menus>
                  </MenuBar>
                  <ToolBar>
                    <items>
                      <Button fx:id="openFileButton" mnemonicParsing="false" text="Open">
                           <graphic>
                              <ImageView fitHeight="25.0" fitWidth="20.0" nodeOrientation="INHERIT" pickOnBounds="true">
                                 <image>
                                    <Image url="@../../media/folder.png" />
                                 </image>
                                 <viewport>
                                    <Rectangle2D />
                                 </viewport>
                              </ImageView>
                           </graphic>
                        </Button>
                        <Button layoutX="10.0" layoutY="13.0" mnemonicParsing="false" prefHeight="33.0" text="Compress">
                           <graphic>
                              <ImageView fitWidth="25.0" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@../../media/compress.png" />
                                 </image>
                              </ImageView>
                           </graphic></Button>
                        <Button layoutX="84.0" layoutY="10.0" mnemonicParsing="false" prefHeight="33.0" text="Decompress">
                           <graphic>
                              <ImageView fitWidth="25.0" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@../../media/decompress.png" />
                                 </image>
                              </ImageView>
                           </graphic>
                        </Button>
                    </items>
                  </ToolBar>
               </children>
            </VBox>
         </top>
      </BorderPane>
   </children>
</AnchorPane>
